{
  "manifest_version": 2,
  "name": "(Unofficial) NU URL translate",
  "version": "1.0",

  "description": "Route URLs such as ejournals through Northwestern's proxy",
  "icons": { "16": "images/library_colored16.png",
             "19": "images/library_colored19.png",
             "32": "images/library_colored32.png",
             "38": "images/library_colored38.png",
             "48": "images/library_colored48.png",
            "128": "images/library_colored128.png",
            "512": "images/library_colored512.png" },

  "browser_action": {
    "default_icon": {
      "19": "images/library_colored19.png",
      "38": "images/library_colored38.png"
    },
    "default_title": "NU URL translate"
  },

  "author": "Klinton Bicknell",
  "background": {
    "scripts": ["nu_url_translate.js"],
    "persistent": false
  },

  "permissions": [
    "tabs",
    "notifications",
    "activeTab"
  ]
}
